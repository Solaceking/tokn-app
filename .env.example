# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Database
DATABASE_URL=postgresql://user:password@host:5432/tokns

# Security
ENCRYPTION_KEY=your-64-character-hex-key

# Stripe - Price IDs (create these in Stripe Dashboard)
STRIPE_SECRET_KEY=sk_live_xxx
STRIPE_WEBHOOK_SECRET=whsec_xxx

# Pro Plan Price IDs
STRIPE_PRO_MONTHLY_PRICE_ID=price_pro_monthly_xxx
STRIPE_PRO_YEARLY_PRICE_ID=price_pro_yearly_xxx

# Ultra Plan Price IDs
STRIPE_ULTRA_MONTHLY_PRICE_ID=price_ultra_monthly_xxx
STRIPE_ULTRA_YEARLY_PRICE_ID=price_ultra_yearly_xxx

# App
NEXT_PUBLIC_APP_URL=https://your-domain.com

# Local Auth (for self-hosted deployments)
NEXT_PUBLIC_USE_LOCAL_AUTH=false
NEXTAUTH_SECRET=your-nextauth-secret

# AI (optional - for AI parser)
OPENAI_API_KEY=sk-xxx
